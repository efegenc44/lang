module Main

variant Unit  { Unit  }
variant Unit2 { Unit2 }

variant C(t) { A(t) B(t) C }

fun g(x : Unit) : Unit = Unit::Unit

fun j(x : Unit2) : Unit = Unit::Unit

fun main() : Unit = (
    match C::C {
        let .C    : Unit::Unit
        let .A(x) : g(x)
        let .B(x) : j(x)
    }

    Unit::Unit
)