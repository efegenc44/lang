module Main

import Nat

fun main() : Nat::Nat = id(foo(
    Nat::Nat::Succ(Nat::Nat::Succ(Nat::Nat::Zero)),
    Nat::Nat::Succ(Nat::Nat::Zero)
))

fun id:(T)(x : T) : T = x

fun foo(x : Nat::Nat, y : Nat::Nat) : Nat::Nat =
    x
        .add(y)
        .mul(Nat::Nat::Succ(Nat::Nat::Succ(Nat::Nat::Succ(Nat::Nat::Zero))))
